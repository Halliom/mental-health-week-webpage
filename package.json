{
	"name": "mental-health-week-webpage",
	"scripts": {
		"start": "next",
		"build": "next build",
		"export": "next export",
		"compile": "npm-run-all -s build export"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ths-mental-health-week/mental-health-week-webpage.git"
	},
	"bugs": {
		"url": "https://github.com/ths-mental-health-week/mental-health-week-webpage/issues"
	},
	"homepage": "https://mentalhealthweek.se/",
	"dependencies": {
		"next": "^8.0.1",
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"styled-components": "^4.1.3"
	},
	"devDependencies": {
		"husky": "^1.3.1",
		"lint-staged": "^8.1.4",
		"prettier": "^1.16.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"singleQuote": true,
		"tabWidth": 4
	},
	"babel": {
		"presets": [
			"next/babel"
		],
		"plugins": [
			[
				"styled-components",
				{
					"ssr": true
				}
			]
		]
	}
}
